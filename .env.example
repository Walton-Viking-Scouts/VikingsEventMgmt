# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Cypress Cloud Configuration (for E2E testing)
CYPRESS_PROJECT_ID=your-cypress-project-id-here
CYPRESS_RECORD_KEY=your-cypress-record-key-here

# API Configuration  
VITE_API_URL=https://vikings-osm-event-manager.onrender.com

# Development
NODE_ENV=development

# OAuth Configuration
VITE_OAUTH_CLIENT_ID=your_oauth_client_id_here

# Sentry Configuration  
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_APP_VERSION=1.0.0

# Testing Configuration
CYPRESS_ENABLE_API_MOCKING=true